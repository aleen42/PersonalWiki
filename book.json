{
    "gitbook": "local",
    "structure": {
        "readme": "README.md"
    },
    "styles": {
        "website": "style/website.css",
        "ebook": "style/ebook.css",
        "pdf": "style/pdf.css",
        "mobi": "style/mobi.css",
        "epub": "style/epub.css"
    },
    "links": {
        "sidebar": {
            "GitHub Project": "https://github.com/aleen42/PersonalWiki"
        }
    },
    "plugins": [
        "github",
        "github-edit",
        "sharing",
        "ga",
        "advanced-emoji",
        "page-footer",
        "page-treeview",
        "ace",
        "resources"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "personal"
        },
        "github": {
            "url": "https://github.com/aleen42/personalwiki"
        },
        "github-edit": {
            "repo": "aleen42/personalwiki"
        },
        "sharing": {
            "facebook": true,
            "twitter": false,
            "google": false,
            "weibo": true,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper",
                "vk"
            ]
        },
        "ga": {
            "token": "UA-75457732-1",
            "configuration": "auto"
        },
        "injection": {
            "js": [
                "src/index.js"
            ]
        },
        "advanced-emoji": {
            "embedEmojis": true
        },
        "page-footer": {
            "isShowIssues": true,
            "wisdom": "More than a coder, more than a designer",
            "copyright": "Copyright © aleen42",
            "style": "symmetrical",
            "repo": "aleen42/PersonalWiki",
            "issueNum": "20",
            "isShowQRCode": true,
            "timeColor": "#a10000",
            "utcOffset": "8",
            "format": "yyyy-MM-dd hh:mm:ss",
            "signature": "Aleen",
            "baseUri": "https://aleen42.github.io/PersonalWiki/",
            "copyrightColor": "#666",
            "description": "update time:"
        },
        "page-treeview": {
            "copyright": "Copyright © aleen42",
            "style": "markdown",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
        "resources": {
            "type": "github",
            "repo": "aleen42/PersonalWiki",
            "branch": "master"
        }
    }
}
